{
  "name": "razzle-graphile",
  "version": "4.0.4",
  "license": "MIT",
  "scripts": {
    "typesync": "typesync --ignoredeps=dev",
    "prestart": "run-s -s db:start",
    "start": "run-p -s watch:db watch:gqlgen watch:server #watch:tsc",
    "build:server": "razzle build --noninteractive",
    "build:schema": "node -r dotenv/config ./node_modules/.bin/postgraphile -X --export-schema-graphql ./src/generated/schema.gql",
    "build": "run-s -s build:schema gqlgen build:server",
    "gqlgen": "gql-gen -e",
    "start:prod": "NODE_ENV=production node -r dotenv/config build/server.js",
    "watch:tsc": "run-s -s 'type-check --watch'",
    "watch:gqlgen": "run-s -s 'gqlgen -w'",
    "watch:debug": "DEBUG=postgraphile:graphql,postgraphile:request,postgraphile:postgres razzle start --inspect",
    "watch:server": "razzle start",
    "watch:db": "run-s -s 'gm watch'",
    "gm": "node -r dotenv/config ./node_modules/.bin/graphile-migrate",
    "db:setup": "node -r dotenv/config scripts/setup.js",
    "db:start": "docker-compose up -d db",
    "db:latest": "run-s -s 'gm watch --once'",
    "db:reset": "run-s -s 'gm reset --erase'",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:dump-schema": "docker-compose run db pg_dump --no-sync --schema-only --no-owner --no-comments --exclude-schema=graphile_migrate --exclude-schema=graphile_worker > ./schema.sql",
    "db:dump-data": "docker-compose run db pg_dump --no-sync --data-only --no-owner --no-comments --exclude-schema=graphile_migrate --exclude-schema=graphile_worker > ./data.sql",
    "generate-env-file": "node scripts/generateEnvFile.js",
    "setup": "run-s -s generate-env-file db:start db:setup db:reset start",
    "lint:js": "eslint --cache --ext=.js,.jsx,.ts,.tsx scripts src worker",
    "lint:css": "stylelint 'src/*/**.css'",
    "lint": "run-p -sc lint:*",
    "lint-fix:css": "run-p -s 'lint:css --fix'",
    "lint-fix:js": "run-p -s 'lint:js --fix'",
    "lint-fix": "run-p -sc lint-fix:*",
    "type-check": "tsc --noEmit --pretty --preserveWatchOutput",
    "test": "razzle test --env=jsdom"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-pubsub": "^4.12.2",
    "@pyramation/postgraphile-plugin-fulltext-filter": "^2.0.0",
    "connect-pg-simple": "^7.0.0",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphile-build": "^4.12.2",
    "graphile-build-pg": "^4.12.2",
    "graphile-migrate": "^1.0.2",
    "graphile-utils": "^4.12.2",
    "graphile-worker": "^0.12.0",
    "graphql": "^15.5.0",
    "helmet": "^4.5.0",
    "html-to-text": "^8.0.0",
    "inquirer": "^8.1.5",
    "lodash": "^4.17.21",
    "mjml": "^4.10.4",
    "modern-normalize": "^1.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.3",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "pg": "^8.7.1",
    "pg-sql2": "^4.12.1",
    "postgraphile": "^4.12.5",
    "postgraphile-plugin-connection-filter": "^2.2.2",
    "postgraphile-remove-foreign-key-fields-plugin": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.1.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@graphql-codegen/typescript-react-apollo": "^3.1.2",
    "@graphql-codegen/typescript-resolvers": "^2.1.2",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7.1.14",
    "@types/connect-pg-simple": "^4.2.2",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/html-to-text": "^8.0.1",
    "@types/inquirer": "^8.1.1",
    "@types/lodash": "^4.14.176",
    "@types/mjml": "^4.7.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.11.2",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-github2": "^1.2.4",
    "@types/pg": "^8.6.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.1.7",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-preset-razzle": "4.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql-tag": "^2.11.0",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.2",
    "prettier": "^2.2.1",
    "razzle": "4.2.2",
    "razzle-dev-utils": "4.2.2",
    "razzle-plugin-graphql": "4.2.2",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.2.4",
    "typesync": "^0.8.0",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "graphql": "^15.5.0",
    "postcss": "^8.3.6"
  }
}
